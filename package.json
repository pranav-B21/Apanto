{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "npm run cleanup && concurrently \"npm run backend\" \"npm run frontend:wait\"",
    "prestart": "npm run ensure-deps && npm run check-python-deps",
    "ensure-deps": "[ ! -d 'node_modules' ] && npm install || true",
    "cleanup": "lsof -ti:8000 | xargs kill -9 2>/dev/null || true; lsof -ti:5173 | xargs kill -9 2>/dev/null || true",
    "check-python-deps": "python3 -c 'import fastapi, uvicorn, pydantic, requests, dotenv, psycopg2' 2>/dev/null || npm run install:python",
    "install:python": "pip3 install -r requirements.txt",
    "backend": "arch -arm64 python3 -m uvicorn backend.main:app --reload --host 127.0.0.1 --port 8000 --app-dir src",
    "frontend:wait": "npm run wait-for-backend && npm run dev",
    "wait-for-backend": "node -e \"const http=require('http');let attempts=0;const check=()=>{http.get('http://localhost:8000/health',res=>{console.log('✅ Backend ready!');process.exit(0)}).on('error',()=>{attempts++;if(attempts<30){setTimeout(check,1000)}else{console.error('❌ Backend failed to start');process.exit(1)}})};check()\"",
    "start": "npm run dev:full",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "python_dependencies": {
    "fastapi": ">=0.68.0",
    "uvicorn": ">=0.15.0",
    "pydantic": ">=1.8.0",
    "requests": ">=2.26.0",
    "python-dotenv": ">=0.19.0",
    "psycopg2-binary": ">=2.9.0",
    "transformers": ">=4.21.0",
    "torch": ">=1.12.0,<2.3.0",
    "accelerate": ">=0.20.0",
    "huggingface_hub": ">=0.15.0",
    "openai": ">=1.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
